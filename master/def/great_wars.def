# 妖怪大戦 妖怪大戦スケジュール テーブル出力定義

inputFile						# 入力ファイル
	${MTC-Developer}\Master\2.0.0\current\data\妖怪大戦\妖怪大戦スケジュール.xlsx

Sheet							# シート名
	Sheet1
Row								# 行範囲
	4-

Format							# 出力フォーマット( ヘッダー,カラム; )
	id,$B; monster_id,$C; monster_scale1,$D; monster_scale2,$E
	tran_y1,$F
	tran_y1_dead,$G
	tran_y2,$H
	dead_motion,$I
	dead_frame,$J
	area_code,$K
	latitude,$L; longitude,$M
	tribe_type1,$N; tribe_type2,$O; max_life,$P
	battle_start_at,$Q,DateTime
	battle_end_at,$R,DateTime
	national_reward_pattern_id,$V; area_reward_pattern_id,$W; personal_reward_pattern_id,$X
	prev_great_war_id,$Y

# エリアーコードデータ csv出力定義
outputFile						# 出力ファイル
	csv/great_wars.csv
Conv

