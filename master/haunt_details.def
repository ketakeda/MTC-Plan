# HauntMaster.xlsx テーブル出力定義

inputFile						# 入力ファイル
	${MTC-Developer}\plan\data\バトル\HauntMaster.xlsx

Sheet							# シート名
	Sheet1
	
Row								# 行範囲
	2-

ValidationCheck					# データを有効／無効をチェックするカラム
	$A

Format							# 出力フォーマット
	id,$A; name,$B; haunt_description,$C; haunt_text,$D
	effect_id,$E; command_id,$F

# エリアーコードデータ csv出力定義
outputFile						# 出力ファイル
	csv/haunt_details.csv

