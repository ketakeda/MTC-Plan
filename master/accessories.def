# 装備アクセサリー テーブル出力定義

inputFile						# 入力ファイル
	${MTC-Developer}\plan\data\MTC リスト DBMaster.xlsx

Sheet							# シート名
	そうび（アクセサリー）リスト
	
Row								# 行範囲
	6-

HeaderDefRow					# ヘッダー判定に使用する行
	4

ValidationCheck					# データを有効／無効をチェックするカラム
	$C

Format							# 出力フォーマット
	id,$C; seq,$S; name,$D
	effect_type_1,$F; effect_type_2,$G
	min_attack,$H; max_attack,$I
	min_magic,$J; max_magic,$K
	min_defense,$L; max_defense,$M
	min_speed,$N; max_speed,$O
#	accessory_group_id
	rank,$Q; mode,$G
	detail,$AK
	description_1,$R;
	#description_2; description_3; description_4

# エリアーコードデータ csv出力定義
outputFile						# 出力ファイル
	csv/accessories.csv

Conv

