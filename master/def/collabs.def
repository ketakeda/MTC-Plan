# wifiコラボイベントリスト テーブル出力定義

inputFile						# 入力ファイル
	${MTC-Developer}\plan\data\MTC リスト DBMaster.xlsx
Sheet							# シート名
	wifiコラボイベントリスト
Row								# 行範囲
	4-

ValidationCheck					# データを有効／無効をチェックするカラム
	$B

Format							# 出力フォーマット( ヘッダー,カラム; )
	id,$B; schedule_id,$C; title,$D; comment,$E
	url,$F; browser_type,$G
	open_time,$H,DateTime; close_time,$I,DateTime
	radius,$J; event_type,$K; condition_type,$L
	recovery_num,$M; cool_time_minutes,$N

# エリアーコードデータ csv出力定義
outputFile						# 出力ファイル
	csv/collabs.csv
Conv

