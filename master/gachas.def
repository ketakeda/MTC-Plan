# ガチャマスタ.xlsx gachas テーブル出力定義

inputFile						# 入力ファイル
	${MTC-Developer}\plan\data\ガチャマスタ.xlsx
Sheet							# シート名
	gachas
Row								# 行範囲
	3-6
Format							# 出力フォーマット( ヘッダー,カラム; )
	id,A; type,B; name,C; order,D
	start_at,E,DateTime; end_at,F,DateTime; banner,G
	attention_text_id,H; description_text_id,I
	stand_id,J

UTF8BOM							# UTF-8 BOM 有無設定
	false

# エリアーコードデータ csv出力定義
outputFile						# 出力ファイル
	csv/gachas.csv
Conv

